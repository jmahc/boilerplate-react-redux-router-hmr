{
  "extends": [
    "airbnb",
    "airbnb-base",
  ],
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "rules": {
    "jsx-a11y/anchor-has-content": 0,
    "react/no-multi-comp": 0,
    "react/forbid-prop-types": 0,
    "react/jsx-filename-extension": 0,
    "import/default": 0,
    "import/extensions": 0,
    "import/named": 0,
    "import/namespace": 0,
    "import/no-duplicates": 0,
    "import/no-extraneous-dependencies": 0,
    "import/no-named-as-default": 2,
    "import/no-unresolved": 0,
    "indent": [
      2,
      2,
      {
        "SwitchCase": 1
      }
    ],
    "no-alert": 0,
    "no-console": 0,
    "no-continue": 0,
    "no-plusplus": 0,
    "no-underscore-dangle": 0,
    "linebreak-style": 0
  },
  "plugins": [
    "react",
    "import"
  ],
  "settings": {
    "import/parser": "babel-eslint",
    "import/resolve": {
      "moduleDirectory": [
        "node_modules",
        "src"
      ]
    }
  },
  "globals": {
    "__CLIENT__": true,
    "__DEVELOPMENT__": true,
    "__DEVTOOLS__": true,
    "__SERVER__": true
  }
}
